<?xml version="1.0" encoding="utf-8"?>
<TableLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  >
  	<TableRow>
  	<TextView android:layout_height="wrap_content" style="@style/common_style_for_20dip"
				android:text="TextView" android:id="@+id/tv_subpoi_tel"
				android:layout_width="fill_parent"
				android:layout_span="3"
				android:layout_marginLeft="20dip"></TextView>
	</TableRow>
	<TableRow>
	<TextView android:layout_width="fill_parent" style="@style/common_style_for_20dip"
		android:text="TextView" android:layout_height="wrap_content"
		android:id="@+id/tv_subpoi_addr" android:layout_span="3"
		android:layout_marginLeft="20dip"></TextView>
	</TableRow>
	<TableRow>
	<TextView android:layout_width="fill_parent" style="@style/common_style_for_20dip"
		android:text="TextView" android:layout_height="wrap_content"
		android:id="@+id/tv_subpoi_district"  android:layout_span="3"
		android:layout_marginLeft="20dip"></TextView>
	</TableRow>
	<TableRow>
			<Button 
				android:layout_height="wrap_content" android:id="@+id/btn_subpoi_fav"
				android:text="@string/searchpoiresult_fav" android:textSize="16dip"
				android:layout_width="90dip"
		    	android:layout_marginLeft="20dip"></Button>
			<Button  android:layout_width="90dip"
				android:layout_height="wrap_content" android:id="@+id/btn_subpoi_del"
				android:text="@string/searchpoiresult_del" android:enabled="false"
				android:textSize="16dip"></Button>
			<Button  android:layout_width="90dip"
				android:layout_height="wrap_content" android:id="@+id/btn_subpoi_edit"
				android:text="@string/searchpoiresult_edit" android:enabled="false"
				android:textSize="16dip"></Button>
		</TableRow>
		<TableRow android:id="@+id/tableRow4" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:layout_gravity="center">
			<Button  android:layout_width="90dip"
				android:layout_height="wrap_content" android:id="@+id/btn_subpoi_location"
				android:text="@string/searchpoiresult_location" android:textSize="16dip"
				 android:layout_marginLeft="20dip"></Button>
			<Button  android:layout_width="90dip"
				android:layout_height="wrap_content" android:id="@+id/btn_subpoi_startPoint"
				android:text="@string/searchpoiresult_startPoint"
				android:textSize="16dip"></Button>
			<Button  android:layout_width="90dip"
				android:layout_height="wrap_content" android:id="@+id/btn_subpoi_endPoint"
				android:text="@string/searchpoiresult_endPoint" android:textSize="16dip"></Button>
		</TableRow>
</TableLayout>
